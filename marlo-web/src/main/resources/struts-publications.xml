<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
  <!-- Add addition packages and configuration here. -->
  <package name="publications" namespace="/publications" extends="marlo-default">

    <!-- Publications not directly linked to a Project Lists -->
    <action name="{crp}/publicationsList" class="org.cgiar.ccafs.marlo.action.BaseAction">
      <interceptor-ref name="homeStack" />
      <result name="input">/WEB-INF/views/publications/publicationsList.ftl</result>
    </action>
    
    <!-- Publication -->
    <action name="{crp}/publication" class="org.cgiar.ccafs.marlo.action.BaseAction">
      <interceptor-ref name="homeStack" />
      <result name="input">/WEB-INF/views/publications/publication.ftl</result>
    </action>
    
    
  </package>
</struts>
