<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
   
  <!-- CRP Plan of Work and Budget (POWB) -->
  <package name="powb" namespace="/powb" extends="marlo-default">
    
    <!-- Delivery -->
    <action name="{crp}/delivery" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbDelivery.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/delivery</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Budget -->
    <action name="{crp}/budget" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbBudget.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/budget</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Collaboration and Integration -->
    <action name="{crp}/collIntegration" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbCollIntegration.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/collIntegration</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Management, Governance and Monitoring, Evaluation, Learning -->
    <action name="{crp}/manGovMonEvaLea" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbManGovMonEvaLea.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/manGovMonEvaLea</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Expected Annual Milestones -->
    <action name="{crp}/expectedMilestones" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbExpectedMilestones.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/expectedMilestones</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Outputs -->
    <action name="{crp}/outputs" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbOutputs.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/outputs</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Contribution of W1W2 Funds -->
    <action name="{crp}/contFunds" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbContFunds.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/contFunds</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Planned Budget -->
    <action name="{crp}/plannedBudget" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbPlannedBudget.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/plannedBudget</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Contribution to SRF Sub-IDOs -->
    <action name="{crp}/contSubIDOs" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbContSubIDOs.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/contSubIDOs</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Expected Annual Milestones -->
    <action name="{crp}/expectedMilestonestable" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbExpectedMilestonestable.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/expectedMilestonestable</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
    <!-- Expected Key Outputs -->
    <action name="{crp}/expectedKeyOutputstable" class="org.cgiar.ccafs.marlo.action.powb.DeliveryAction">
      <interceptor-ref name="editSynthesisStack" />
      <result name="input">/WEB-INF/views/powb/powbExpectedKeyOutputstable.ftl</result>
      <result name="success" type="redirectAction">
        <param name="actionName">${crpSession}/expectedKeyOutputstable</param>
        <param name="liaisonInstitutionID">${liaisonInstitutionID}</param>
        <param name="edit">true</param>
      </result>
    </action>
    
  </package>
</struts>
