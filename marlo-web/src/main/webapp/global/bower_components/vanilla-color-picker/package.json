{
  "name": "vanilla-color-picker",
  "version": "0.1.4",
  "description": "Minimalistic color picker without dependencies",
  "main": "vanilla-color-picker.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node_modules/karma/bin/karma start --single-run=true",
    "start": "node node_modules/minifier/index.js --output dist/vanilla-color-picker.min.js src/vanilla-color-picker.js && cp src/vanilla-color-picker.js dist/vanilla-color-picker.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miroshko/vanilla-color-picker.git"
  },
  "keywords": [
    "color",
    "picker"
  ],
  "author": "O. Miroshko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miroshko/vanilla-color-picker/issues"
  },
  "homepage": "https://github.com/miroshko/vanilla-color-picker",
  "dependencies": {
    "karma": "^0.12.32",
    "karma-effroi": "0.0.0",
    "minifier": "^0.6.2"
  },
  "devDependencies": {
    "jasmine-core": "^2.3.4",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.1.5",
    "karma-jasmine": "^0.3.5",
    "karma-opera-launcher": "^0.1.0",
    "karma-requirejs": "^0.2.2",
    "karma-safari-launcher": "^0.1.1",
    "requirejs": "^2.1.17"
  }
}
