<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.cgiar.ccafs.marlo.data.model.SDG" table="sustainable_development_goals"
    optimistic-lock="none">
      <id name="id" type="java.lang.Long">
      <column name="id" />
      <generator class="identity" />
    </id>
    <property name="smoCode" type="string">
      <column name="smo_code" length="50" not-null="true" />
    </property>
    <property name="shortName" type="string">
      <column name="short_name" length="100" not-null="true" />
    </property>
    <property name="fullName" type="string">
      <column name="full_name"  not-null="true" />
    </property>
    <property name="description" type="string">
      <column name="description" length="800" not-null="true" />
    </property>
  </class>
</hibernate-mapping>