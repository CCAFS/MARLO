<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.cgiar.ccafs.marlo.data.model.ProjectExpectedStudyTag" 
  table="project_expected_study_tags" optimistic-lock="none">
    <id name="id" type="java.lang.Long">
      <column name="id" />
    </id>
    <property name="tagName" type="string">
        <column name="tag_name" sql-type="TEXT" />
    </property> 
    <property name="tagDescription" type="string">
        <column name="tag_description" sql-type="TEXT" />
    </property> 
  </class>
</hibernate-mapping>
